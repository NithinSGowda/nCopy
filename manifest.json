{
    "manifest_version": 2,
    "name": "nCopy",
    "version": "1.0.0",
    "description": "By Nithin S",
    "icons": {
      "128": "icon/icon.png"
    },
    "browser_action": {
      "default_icon":"icon/icon.png",
      "default_title": "nCopy",
      "default_popup": "html/popup.html"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["js/contentscript.js"]
    }],
    "permissions": [
      "storage",
      "activeTab",
      "cookies",
      "https://nbot.live/"
    ]
  }
  
  